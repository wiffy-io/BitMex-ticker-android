<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">



    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
            <corners
                    android:radius="5dip"/>
            <gradient
                    android:startColor="#335533"
                    android:endColor="#335533"
            />
        </shape>
    </item>

    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners
                    android:radius="5dip"/>
            <gradient
                    android:startColor="#335533"
                    android:endColor="#335533"
            />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners
                    android:radius="5dip"/>
            <gradient
                    android:startColor="#66aa66"
                    android:endColor="#66aa66"
            />
        </shape>
    </item>

</selector>